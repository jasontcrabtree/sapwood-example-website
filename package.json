    "gatsby-plugin-react-helmet-async": "^1.1.0",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-source-prismic": "^3.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
